@model List<PersonViewModel>


<div class="col-lg-10" id="DisplayPeopleTable">
        @foreach (PersonViewModel item in Model)
        {
            <div class="row col-lg-12">
                <p>| @item.Id | @item.Name | @item.Phonenumber | @item.CityName | @Html.ActionLink("Delete", "DeleteFromList", new {DeleteId = item.Id}) |</p>
            </div>
        }
</div>

